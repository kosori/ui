{
  "name": "@kosori/cli",
  "version": "0.1.1",
  "description": "CLI for @kosori",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "bin": "./dist/index.js",
  "exports": "./dist/index.js",
  "license": "MIT",
  "author": {
    "name": "Alexis Guzman",
    "url": "https://www.codingcodax.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kosori/ui.git",
    "directory": "packages/cli"
  },
  "keywords": [
    "components",
    "ui",
    "tailwind",
    "radix-ui",
    "cli"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .turbo node_modules",
    "dev": "pnpx tsx src/index.ts",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "pub:beta": "pnpm build && pnpm publish --no-git-checks --access public --tag beta",
    "pub:next": "pnpm build && pnpm publish --no-git-checks --access public --tag next",
    "pub:release": "pnpm build && pnpm publish --access public"
  },
  "devDependencies": {
    "@kosori/eslint-config": "workspace:*",
    "@kosori/prettier-config": "workspace:*",
    "@kosori/tsconfig": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsup": "^8.2.4",
    "typescript": "catalog:"
  },
  "prettier": "@kosori/prettier-config",
  "dependencies": {
    "@antfu/ni": "^0.23.0",
    "@clack/core": "^0.3.4",
    "@clack/prompts": "^0.7.0",
    "commander": "^12.1.0",
    "consola": "^3.2.3",
    "cosmiconfig": "^9.0.0",
    "execa": "^9.3.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "picocolors": "^1.0.1",
    "ts-morph": "^23.0.0",
    "tsconfig-paths": "^4.2.0"
  }
}
