{
  "name": "@kosori/cli",
  "version": "0.2.1",
  "description": "CLI for @kosori",
  "keywords": [
    "cli",
    "components",
    "radix-ui",
    "tailwind",
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kosori/ui.git",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "author": "Alexis Guzman (https://www.codingcodax.dev)",
  "type": "module",
  "exports": "./dist/index.js",
  "bin": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:silent": "tsup --silent",
    "clean": "rm -rf .turbo node_modules",
    "dev": "pnpx tsx src/index.ts",
    "dev:node": "pnpm build:silent && pnpm start",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "pub:beta": "pnpm build && pnpm publish --no-git-checks --access public --tag beta",
    "pub:next": "pnpm build && pnpm publish --no-git-checks --access public --tag next",
    "pub:release": "pnpm build && pnpm publish --access public",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@kosori/prettier-config",
  "dependencies": {
    "@antfu/ni": "^24.4.0",
    "@clack/core": "^0.5.0",
    "@clack/prompts": "^0.10.1",
    "@prettier/sync": "^0.5.5",
    "commander": "^14.0.0",
    "consola": "^3.4.2",
    "cosmiconfig": "^9.0.0",
    "execa": "^9.5.3",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "picocolors": "^1.1.1",
    "ts-morph": "^26.0.0",
    "tsconfig-paths": "^4.2.0",
    "type-fest": "^4.41.0"
  },
  "devDependencies": {
    "@kosori/eslint-config": "workspace:*",
    "@kosori/prettier-config": "workspace:*",
    "@kosori/tsconfig": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsup": "^8.5.0",
    "typescript": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
